{
  "name": "magic-container",
  "version": "0.0.1",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "license": "MIT",
  "scripts": {
    "start": "ts-node ./src/index.ts",
    "format": "prettier --list-different '**/*.{ts,json,md, gql}'",
    "format:fix": "prettier --write '**/*.{ts,json,md,gql}'",
    "tslint": "tslint -p ./tsconfig.json -e 'node_modules/**/*' -e '**/node_modules/**/*' '**/*.ts'",
    "tslint:fix": "tslint --fix --format stylish -p ./tsconfig.json -e 'node_modules/**/*' -e '**/node_modules/**/*' '**/*.ts'",
    "tsc": "tsc --noEmit",
    "lint": "yarn format && yarn tslint && yarn tsc",
    "lint:fix": "yarn format:fix && yarn tslint:fix && yarn tsc",
    "test:mocha": "NODE_ENV=test mocha --require ts-node/register --require ./test/setup.ts '**/*.spec.ts'",
    "test": "yarn lint && yarn test:mocha",
    "test:fix": "yarn lint:fix && yarn test:mocha",
    "build": "rm -rf ./lib && tsc -p ./tsconfig.prod.json --declaration && rm ./lib/*.private.d.ts",
    "prepublishOnly": "yarn test && yarn build"
  },
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/lodash": "^4.14.108",
    "@types/mocha": "^5.1.1",
    "@types/node": "^10.0.6",
    "@types/sinon": "^4.3.2",
    "@types/sinon-chai": "^2.7.30",
    "chai": "^4.1.2",
    "mocha": "^5.1.1",
    "prettier": "^1.12.1",
    "sinon": "^5.0.7",
    "sinon-chai": "^3.0.0",
    "ts-node": "^6.0.0",
    "tslint": "^5.9.1",
    "tslint-language-service": "^0.9.9",
    "typescript": "^2.8.1",
    "typestrict": "^0.0.8"
  },
  "dependencies": {}
}
